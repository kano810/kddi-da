{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "model": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "model": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image",
              "fields": [
                {
                  "name": "imageAlt",
                  "selector": "img[alt]"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Accordion",
          "id": "accordion",
          "model": "accordion",
          "filter": "accordion",
          "plugins": {
            "da": {
              "name": "accordion",
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "model": "accordion-item",
          "plugins": {
            "da": {
              "name": "accordion-item",
              "rows": 2,
              "columns": 0,
              "fields": [
                {
                  "name": "summary",
                  "selector": "div:nth-child(1)"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Cards",
          "id": "cards",
          "model": "cards",
          "filter": "cards",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Card",
          "id": "card",
          "model": "card",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2,
              "fields": [
                {
                  "name": "image",
                  "selector": "div:nth-child(1)>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div:nth-child(1)>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "model": "carousel",
          "filter": "carousel",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "model": "carousel-item",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "backgroundImage",
                  "selector": "div:nth-child(1)>picture>img[src]"
                },
                {
                  "name": "backgroundImageAlt",
                  "selector": "div:nth-child(1)>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Columns",
          "id": "columns",
          "model": "columns",
          "filter": "columns",
          "plugins": {
            "da": {
              "name": "columns",
              "rows": 1,
              "columns": 2,
              "behaviour": "columns"
            }
          }
        },
        {
          "title": "Columns Row",
          "id": "columns-row",
          "model": "columns-row",
          "filter": "columns-row",
          "plugins": {
            "da": {
              "name": "columns-row",
              "behaviour": "columns-row"
            }
          }
        },
        {
          "title": "Columns Cell",
          "id": "columns-cell",
          "model": "columns-cell",
          "filter": "columns-cell",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>",
              "behaviour": "columns-cell"
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "model": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>",
              "fields": [
                {
                  "name": "url",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "urlText",
                  "selector": "div>div>p>a"
                }
              ]
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "model": "hero",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture><h1></h1></div></div></div>",
              "fields": [
                {
                  "name": "image",
                  "selector": "div>div>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div>div>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div>div>h1"
                }
              ]
            }
          }
        },
        {
          "title": "Quote",
          "id": "quote",
          "model": "quote",
          "plugins": {
            "da": {
              "name": "quote",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "quote",
                  "selector": "div:nth-child(1)>div"
                },
                {
                  "name": "author",
                  "selector": "div:nth-child(2)>div"
                }
              ]
            }
          }
        },
        {
          "title": "Search",
          "id": "search",
          "model": "search",
          "plugins": {
            "da": {
              "name": "search",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "searchUrl",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "searchUrlText",
                  "selector": "div>div>p>a"
                }
              ]
            }
          }
        },
        {
          "title": "Tabs",
          "id": "tabs",
          "model": "tabs",
          "filter": "tabs",
          "plugins": {
            "da": {
              "columns": 1,
              "rows": 2
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "model": "tabs-item",
          "plugins": {
            "da": {
              "columns": 0,
              "rows": 2,
              "fields": [
                {
                  "name": "name",
                  "selector": "div:nth-child(1)"
                },
                {
                  "name": "content",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Video",
          "id": "video",
          "model": "video",
          "plugins": {
            "da": {
              "name": "video",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "videoUrl",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "videoUrlText",
                  "selector": "div>div>p>a"
                },
                {
                  "name": "posterImage",
                  "selector": "div>div>picture>img[src]"
                }
              ]
            }
          }
        }
      ]
    }
  ]
}